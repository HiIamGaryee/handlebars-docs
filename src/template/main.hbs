      <script>
         // Initialize all functionality when DOM is loaded
           document.addEventListener('DOMContentLoaded', function() {
               initHamburgerMenu();
               initDropdownOverflowGuards();
               initFAQ();
           });
         
         // FAQ Functionality
         function initFAQ() {
             document.querySelectorAll('.faq__question').forEach(question => {
                 question.addEventListener('click', () => {
                     const faqItem = question.parentElement;
                     const isActive = faqItem.classList.contains('active');
           // Close others
                     document.querySelectorAll('.faq__block').forEach(item => {
             if (item !== faqItem) item.classList.remove('active');
           });
           // Toggle current
           faqItem.classList.toggle('active', !isActive);
           question.setAttribute('aria-expanded', String(!isActive));
                 });
             });
         }
         // Hamburger Menu Functionality
         function initHamburgerMenu() {
             const headerHamburgerBtn = document.querySelector('[data-header-menu]');
             const footerHamburgerBtn = document.querySelector('.bottom-menu-wrap .btn-hamburger, .bottom-menu-wrap [data-btn-main-menu]');
             const headerHamburgerSubmenu = document.getElementById('header-hamburger-submenu');
             const mobileDrawer = document.getElementById('mobile-drawer');
             const drawerOverlay = mobileDrawer?.querySelector('.drawer__overlay');
             const drawerClose = mobileDrawer?.querySelector('.drawer__close');
             const sidebarMenu = document.getElementById('sidebar-menu');
             const sidebarOverlay = document.getElementById('sidebarOverlay');
             const sidebarCloseBtn = document.getElementById('sidebar-close');
             const body = document.body;
         
             // Header Hamburger Button - Toggle Sidebar (Mobile)
             if (headerHamburgerBtn && sidebarMenu) {
                 headerHamburgerBtn.addEventListener('click', (e) => {
                     e.preventDefault();
                     e.stopPropagation();
                     
                     // Toggle sidebar
                     const isOpen = sidebarMenu.classList.contains('active');
                     if (isOpen) {
                         closeSidebar();
                     } else {
                         openSidebar();
                     }
                 });
             }
             
             // Close sidebar when overlay is clicked
             if (sidebarOverlay) {
                 function handleOverlayClickMobile(e) {
                     e.preventDefault();
                     e.stopPropagation();
                     closeSidebar();
                 }
                 sidebarOverlay.addEventListener('click', handleOverlayClickMobile);
                 sidebarOverlay.addEventListener('mousedown', handleOverlayClickMobile);
             }
             
             // Close sidebar when close button is clicked
             if (sidebarCloseBtn) {
                 sidebarCloseBtn.addEventListener('click', () => {
                     closeSidebar();
                 });
             }
             
             function openSidebar() {
                 if (sidebarMenu) sidebarMenu.classList.add('active');
                 if (sidebarOverlay) sidebarOverlay.classList.add('active');
                 if (headerHamburgerBtn) headerHamburgerBtn.classList.add('is-open');
                 body.classList.add('modal-open');
             }
             
             function closeSidebar() {
                 if (sidebarMenu) sidebarMenu.classList.remove('active');
                 if (sidebarOverlay) sidebarOverlay.classList.remove('active');
                 if (headerHamburgerBtn) headerHamburgerBtn.classList.remove('is-open');
                 body.classList.remove('modal-open');
             }
         
             // Footer Hamburger Button (Mobile only - mobile drawer)
             if (footerHamburgerBtn) {
                 footerHamburgerBtn.addEventListener('click', (e) => {
                     e.preventDefault();
                     e.stopPropagation();
                     
                     // Only work on mobile
                     if (window.innerWidth <= 768) {
                         footerHamburgerBtn.classList.toggle('is-open');
                         if (mobileDrawer) {
                             const isHidden = mobileDrawer.getAttribute('aria-hidden') === 'true';
                             if (isHidden) {
                                 mobileDrawer.setAttribute('aria-hidden', 'false');
                                 body.classList.add('modal-open');
                             } else {
                                 mobileDrawer.setAttribute('aria-hidden', 'true');
                                 body.classList.remove('modal-open');
                             }
                         }
                     }
                 });
             }
         
             // Close main drawer when overlay is clicked (mobile)
             if (drawerOverlay) {
                 drawerOverlay.addEventListener('click', () => {
                     closeMainDrawer();
                 });
             }
         
             // Close main drawer when close button is clicked (mobile)
             if (drawerClose) {
                 drawerClose.addEventListener('click', () => {
                     closeMainDrawer();
                 });
             }
         
             // Handle submenu drawer triggers (mobile)
             const submenuTriggers = document.querySelectorAll('[data-submenu-drawer]');
             submenuTriggers.forEach(trigger => {
                 trigger.addEventListener('click', (e) => {
                     e.preventDefault();
                     e.stopPropagation();
                     
                     const drawerIndex = trigger.getAttribute('data-submenu-drawer');
                     const submenuDrawer = document.getElementById(`submenu-drawer-${drawerIndex}`);
                     
                     if (submenuDrawer) {
                         // Hide main drawer and show submenu drawer
                         mobileDrawer.setAttribute('aria-hidden', 'true');
                         submenuDrawer.setAttribute('aria-hidden', 'false');
                     }
                 });
             });
         
             // Handle all submenu drawers (mobile)
             const submenuDrawers = document.querySelectorAll('.drawer--sub');
             submenuDrawers.forEach(drawer => {
                 const drawerOverlay = drawer.querySelector('.drawer__overlay');
                 const drawerClose = drawer.querySelector('.drawer__close');
                 const drawerBack = drawer.querySelector('.drawer__back');
         
                 // Close submenu drawer when overlay is clicked
                 if (drawerOverlay) {
                     drawerOverlay.addEventListener('click', () => {
                         closeSubmenuDrawer(drawer);
                     });
                 }
         
                 // Close submenu drawer when close button is clicked
                 if (drawerClose) {
                     drawerClose.addEventListener('click', () => {
                         closeSubmenuDrawer(drawer);
                     });
                 }
         
                 // Go back to main drawer when back button is clicked
                 if (drawerBack) {
                     drawerBack.addEventListener('click', () => {
                         closeSubmenuDrawer(drawer);
                         // Show main drawer again
                         mobileDrawer.setAttribute('aria-hidden', 'false');
                     });
                 }
             });
         
             // Close drawers when pressing Escape key
             document.addEventListener('keydown', (e) => {
                 if (e.key === 'Escape') {
                     if (headerHamburgerSubmenu && headerHamburgerSubmenu.style.display === 'block') {
                         headerHamburgerSubmenu.style.display = 'none';
                         headerHamburgerBtn?.classList.remove('is-open');
                     } else if (sidebarMenu && sidebarMenu.classList.contains('active')) {
                         // Close sidebar if open
                         closeSidebar();
                     } else if (mobileDrawer && mobileDrawer.getAttribute('aria-hidden') === 'false') {
                         // Close any open sub-drawers first
                         document.querySelectorAll('.drawer--sub[aria-hidden="false"]').forEach(subDrawer => {
                             subDrawer.setAttribute('aria-hidden', 'true');
                         });
                         closeMainDrawer();
                     }
                 }
             });
         
             function closeMainDrawer() {
                 if (!mobileDrawer) return;
                 headerHamburgerBtn?.classList.remove('is-open');
                 footerHamburgerBtn?.classList.remove('is-open');
                 mobileDrawer.setAttribute('aria-hidden', 'true');
                 body.classList.remove('modal-open');
             }
         
             function closeSubmenuDrawer(drawer) {
                 if (!drawer) return;
                 drawer.setAttribute('aria-hidden', 'true');
             }
         
             // Close when clicking any link inside the mobile drawer
             const panel = mobileDrawer?.querySelector('.drawer__panel');
             panel?.addEventListener('click', e => {
                 const a = e.target.closest('a[href]');
                 if (a) closeMainDrawer();
             });
             
             // Close sidebar when clicking any link inside it
             if (sidebarMenu) {
                 sidebarMenu.addEventListener('click', e => {
                     const a = e.target.closest('a[href]');
                     if (a) closeSidebar();
                 });
             }
         }

         // Ensure dropdowns don't overflow viewport (x/y and top/bottom)
         function initDropdownOverflowGuards() {
             const submenus = Array.from(document.querySelectorAll('.main-menu__item .main-submenu'));
             if (!submenus.length) return;

             const recompute = () => {
                 submenus.forEach(menu => {
                     menu.classList.remove('is-align-right', 'is-drop-up', 'is-scrollable');
                     menu.style.maxHeight = '';

                     const parent = menu.closest('.main-menu__item');
                     if (!parent) return;

                     const originalDisplay = menu.style.display;
                     const computedHidden = getComputedStyle(menu).display === 'none';
                     if (computedHidden) {
                         menu.style.visibility = 'hidden';
                         menu.style.display = 'block';
                     }

                     const rect = menu.getBoundingClientRect();
                     const vw = window.innerWidth;
                     const vh = window.innerHeight;

                     if (rect.right > vw - 8) {
                         menu.classList.add('is-align-right');
                     }
                     const rectAfterX = menu.getBoundingClientRect();
                     if (rectAfterX.left < 8) {
                         menu.classList.remove('is-align-right');
                         menu.style.maxWidth = Math.max(240, Math.floor(vw - 16)) + 'px';
                     } else {
                         menu.style.maxWidth = '';
                     }

                     if (rect.bottom > vh - 8) {
                         menu.classList.add('is-drop-up');
                     }

                     const finalRect = menu.getBoundingClientRect();
                     const availableBelow = vh - 12 - Math.max(0, finalRect.top);
                     const availableAbove = Math.max(0, finalRect.bottom) - 12;
                     const available = menu.classList.contains('is-drop-up') ? availableAbove : availableBelow;
                     if (finalRect.height > available) {
                         menu.classList.add('is-scrollable');
                         menu.style.maxHeight = Math.floor(Math.max(220, available)) + 'px';
                     }

                     if (computedHidden) {
                         menu.style.display = originalDisplay || '';
                         menu.style.visibility = '';
                     }
                 });
             };

             document.querySelectorAll('.main-menu__item').forEach(item => {
                 item.addEventListener('mouseenter', recompute, { passive: true });
                 item.addEventListener('focusin', recompute, { passive: true });
             });
             window.addEventListener('resize', recompute, { passive: true });
             window.addEventListener('scroll', recompute, { passive: true });
         }
         
         // Cookie utility functions
         function setCookie(name, value, days) {
             const expires = new Date();
             expires.setTime(expires.getTime() + (days * 24 * 60 * 60 * 1000));
             document.cookie = name + '=' + value + ';expires=' + expires.toUTCString() + ';path=/';
         }
         
         function getCookie(name) {
             const nameEQ = name + "=";
             const ca = document.cookie.split(';');
             for(let i = 0; i < ca.length; i++) {
                 let c = ca[i];
                 while (c.charAt(0) === ' ') c = c.substring(1, c.length);
                 if (c.indexOf(nameEQ) === 0) return c.substring(nameEQ.length, c.length);
             }
             return null;
         }

        // Enhanced Sidebar Toggle Functionality
        (function() {
          const sidebar = document.getElementById('sidebar-menu');
          let overlay = document.getElementById('sidebarOverlay');
          const menuBtn = document.querySelector('[data-btn-main-menu]');
          const closeBtn = document.getElementById('sidebar-close');
          const banner = document.getElementById('brandAmbassadorBanner');
          
          if (!sidebar || !menuBtn) return;
          
          // Create overlay if it doesn't exist
          if (!overlay) {
            overlay = document.createElement('div');
            overlay.id = 'sidebarOverlay';
            overlay.className = 'sidebar-overlay';
            document.body.appendChild(overlay);
          }
          
          function openSidebar() {
            sidebar.classList.add('active');
            overlay.classList.add('active');
            if (banner) banner.classList.add('active');
            document.body.style.overflow = 'hidden';
          }
          
          function closeSidebar() {
            sidebar.classList.remove('active');
            overlay.classList.remove('active');
            if (banner) banner.classList.remove('active');
            document.body.style.overflow = '';
          }
          
          menuBtn.addEventListener('click', function(e) {
            e.preventDefault();
            if (sidebar.classList.contains('active')) {
              closeSidebar();
            } else {
              openSidebar();
            }
          });
          
          if (closeBtn) {
            closeBtn.addEventListener('click', closeSidebar);
          }
          
          // Close sidebar when clicking on overlay - use both click and mousedown for reliability
          function handleOverlayClick(e) {
            e.preventDefault();
            e.stopPropagation();
            closeSidebar();
          }
          
          overlay.addEventListener('click', handleOverlayClick);
          overlay.addEventListener('mousedown', handleOverlayClick);
          
          // Handle clicks outside sidebar (on document level) - use bubble phase
          function handleDocumentClick(e) {
            // Only handle if sidebar is active
            if (!sidebar || !sidebar.classList.contains('active')) return;
            
            // Don't close if clicking inside sidebar
            if (sidebar.contains(e.target)) return;
            
            // Don't close if clicking on menu button
            if (menuBtn && menuBtn.contains(e.target)) return;
            
            // Close sidebar for any click outside (including overlay)
            closeSidebar();
          }
          
          // Use bubble phase so expandable menu handler can stop it if needed
          document.addEventListener('click', handleDocumentClick, false);
          
          // Close on escape key
          document.addEventListener('keydown', function(e) {
            if (e.key === 'Escape' && sidebar.classList.contains('active')) {
              closeSidebar();
            }
          });
        })();

        // Handle expandable menu items - simple and reliable
        document.addEventListener('click', function(e) {
          // Check if click is on a sidebar expandable menu header
          const header = e.target.closest('[data-toggle-submenu]');
          if (!header) return;
          
          const sidebar = document.getElementById('sidebar-menu');
          if (!sidebar || !sidebar.contains(header)) return;
          
          e.preventDefault();
          e.stopPropagation();
          const parent = header.closest('.sidebar-menu-item-expandable');
          if (parent) {
            parent.classList.toggle('is-open');
          }
        }, true);

        // Back to Top Button Functionality
        (function() {
          const backToTopBtn = document.getElementById('backToTopBtn');
          if (!backToTopBtn) return;
          
          const scrollThreshold = {{back_to_top.scroll_threshold}} || 50;
          
          function checkScroll() {
            if (window.pageYOffset > scrollThreshold) {
              backToTopBtn.style.display = 'block';
              setTimeout(() => {
                backToTopBtn.style.opacity = '1';
              }, 10);
            } else {
              backToTopBtn.style.opacity = '0';
              setTimeout(() => {
                backToTopBtn.style.display = 'none';
              }, 300);
            }
          }
          
          // Initial check
          checkScroll();
          
          // Check on scroll
          window.addEventListener('scroll', checkScroll, { passive: true });
          
          // Scroll to top on click
          backToTopBtn.addEventListener('click', function() {
            window.scrollTo({
              top: 0,
              behavior: 'smooth'
            });
          });
        })();
         
      </script>
 
