/* Viewport height normalization for mobile browsers */
:root{
  --toolbar-height: 70px;
  --safe-bottom-legacy: constant(safe-area-inset-bottom);
  --safe-bottom: env(safe-area-inset-bottom);
}

/* Prefer dynamic viewport units where supported */
@supports (height: 100dvh){
  html, body{ min-height: 100dvh; }
  .main{ min-height: calc(100dvh - (var(--toolbar-height) + var(--safe-bottom))); }
}

/* Fallback for older browsers */
@supports not (height: 100dvh){
  html, body{ min-height: 100vh; }
  .main{ min-height: calc(100vh - (var(--toolbar-height) + var(--safe-bottom-legacy))); }
}



