      <script>
         (function () {
           var css = {{#if new_css_function}}{{{json new_css_function}}}{{else}}""{{/if}};
           var html = {{#if new_html_function}}{{{json new_html_function}}}{{else}}""{{/if}};
         
           if (css) {
             var s = document.createElement("style");
             s.id = "user-extra-css";
             s.appendChild(document.createTextNode(css));
             document.head.appendChild(s);    
           }
         
           if (html) {
             var hook = document.getElementById("user-extra-html");
             if (!hook) {
               hook = document.createElement("div");
               hook.id = "user-extra-html";
               var footer = document.querySelector("footer");
               (footer ? footer.parentNode.insertBefore(hook, footer) : document.body.appendChild(hook));
             }
             hook.innerHTML = html;   
           }
         })();
    </script>
       <script>
  document.addEventListener('DOMContentLoaded', function () {
    const menu = document.querySelector('.bottom-menu-wrap');
    if (!menu) return;

    // Force the bar to stay fixed and visible
    Object.assign(menu.style, {
      position: 'fixed',
      left: '0',
      right: '0',
      bottom: '0',
      zIndex: '999',
      transform: 'translateY(0)',
      transition: 'none',
      display: 'flex',
      pointerEvents: 'auto'
    });

    // Disable any scroll-based hide/show behavior
    const stopScrollScripts = () => {
      menu.style.transform = 'translateY(0)';
    };
    window.addEventListener('scroll', stopScrollScripts, { passive: true });
  });
</script>
